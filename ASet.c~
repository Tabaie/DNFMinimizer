int Contains(ASet *a, ASet *b)
{
	if (*a==*b)	//IF THEY ARE EQUAL, FAVOR a, THE ONE ALREADY IN THE CHAIN
		return -1;

	if ( (*a^*b) == (*a-*b))
		return 1;
	
	if ( (*a^*b) == (*b-*a) )
		return -1;

	return 0;
}

void Add(ASet *a, int itemIndex)
{
	*a|= 1<< itemIndex;
}

void Remove(ASet *a, int itemIndex)
{
	*a&= -1 ^ (1<<itemIndex);
}
