void PrintSet(ASet a)
{
	int i;
	for (i=0; i< sizeof(ASet)*8; i++)
	{
		if (a%2)
			printf("_%d", i);
			
		a/=2;
	}
	
	printf(" ");
}

void PrintSet_IncNeg(ASet)	//Print a set with negated literals
{
	int i;
	for (i=0; i< sizeof(ASet)*4; i++)
	{
		if (a%2)
			printf("_%d", i);
			
		a/=2;
	}

	for (i=0; i< sizeof(ASet)*4; i++)
	{
		if (a%2)
			printf("_~%d", i);
			
		a/=2;
	}

	
	printf(" ");

}
